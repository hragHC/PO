   <body>
       <iframe id="main1" src = "https://htmlpreview.github.io/?https://github.com/hragHC/PO/blob/main/poform1" 
        width = "1522" height = "725" style="border:0;">
      </iframe>
      <script>
          var main1 = document.getElementById("main1");
            if (window.innerWidth <window.innerHeight ){ modeIndex=1; phoneMode(); }
            if (window.innerWidth >= 1108 && window.innerWidth > window.innerHeight){ modeIndex=1; desktopMode(); }
            function desktopMode(){
                main1.height = "725";
            }
            function phoneMode(){
                main1.style.height = (0.75*window.innerHeight)+"px";
            }
      </script>
   </body>
	
</html>
